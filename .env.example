# Application
APP_NAME=Enterprise AI Agent
APP_VERSION=1.0.0
ENVIRONMENT=development
DEBUG=false
LOG_LEVEL=INFO

# Server
HOST=0.0.0.0
PORT=8000
WORKERS=4

# Snowflake (OLAP)
SNOWFLAKE_ACCOUNT=your_account
SNOWFLAKE_USER=your_user
SNOWFLAKE_PASSWORD=your_password
SNOWFLAKE_DATABASE=ANALYTICS_DB
SNOWFLAKE_SCHEMA=PUBLIC
SNOWFLAKE_WAREHOUSE=COMPUTE_WH
SNOWFLAKE_ROLE=AI_AGENT_ROLE
SNOWFLAKE_POOL_SIZE=20
SNOWFLAKE_MAX_OVERFLOW=10

# Checkpointing (memory, redis, postgres)
CHECKPOINT_BACKEND=memory
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# LLM Provider (OAuth)
LLM_API_URL=https://enterprise-llm.com/v1/chat/completions
LLM_OAUTH_TOKEN_URL=https://auth.enterprise.com/oauth/token
LLM_CLIENT_ID=your_client_id
LLM_CLIENT_SECRET=your_client_secret
LLM_MODEL=gpt-4
LLM_MAX_TOKENS=4096
LLM_TEMPERATURE=0.7
LLM_TIMEOUT=30

# S3 Storage
S3_ENDPOINT_URL=https://s3.customer-cloud.com
S3_ACCESS_KEY_ID=your_access_key
S3_SECRET_ACCESS_KEY=your_secret_key
S3_BUCKET_NAME=enterprise-ai-files
S3_REGION=us-east-1

# CopilotKit (Optional)
COPILOTKIT_ENABLED=true
COPILOTKIT_STREAMING=true

# Streaming
ENABLE_STREAMING=true
STREAM_TIMEOUT=300

# Prompts
PROMPTS_DIR=./prompts

# Authentication
AUTH_USERID_HEADER=X-User-ID
AUTH_EMAIL_HEADER=X-User-Email
AUTH_ROLE_HEADER=X-User-Role

# LangSmith
LANGCHAIN_TRACING_V2=true
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
LANGCHAIN_API_KEY=your_langsmith_key
LANGCHAIN_PROJECT=enterprise-ai-agent

# CORS
CORS_ORIGINS=["http://localhost:3000"]
CORS_ALLOW_CREDENTIALS=true
